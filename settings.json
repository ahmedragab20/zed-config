{
    "auto_update": false,
    "edit_predictions": {
        "mode": "subtle",
        "enabled_in_text_threads": false
    },
    "tabs": {
        "git_status": true
    },
    "tab_size": 4,
    "tab_bar": {
        "show_nav_history_buttons": false,
        "show_tab_bar_buttons": false,
        "show": false
    },
    "toolbar": {
        "breadcrumbs": true,
        "quick_actions": false,
        "selections_menu": false,
        "code_actions": false
    },
    "cursor_blink": false,
    "vertical_scroll_margin": 5,
    "max_tabs": 7,
    "vim_mode": true,
    "vim": {
        "use_system_clipboard": "always",
        "use_smartcase_find": true,
        "highlight_on_yank_duration": 200
    },
    "command_aliases": {
        "W": "w",
        "Wq": "wq",
        "Q": "q",
        "Qa": "qa",
        "Qwa": "qwa",
        "QWa": "qwa"
    },
    "relative_line_numbers": true,
    "hide_mouse": "on_typing",
    "current_line_highlight": "all",
    "scrollbar": {
        "show": "never"
    },
    "gutter": {
        "line_numbers": true,
        "runnables": true,
        "breakpoints": true,
        "folds": true,
        "min_line_number_digits": 4
    },
    "indent_guides": {
        "enabled": true,
        "line_width": 1,
        "active_line_width": 1,
        "coloring": "fixed",
        "background_coloring": "disabled"
    },
    "git": {
        "inline_blame": {
            "enabled": true,
            "min_column": 0,
            "show_commit_summary": false
        },
        "hunk_style": "unstaged_hollow"
    },
    "project_panel": {
        "dock": "right",
        "scrollbar": null,
        "button": false,
        "sticky_scroll": false,
        "folder_icons": false,
        "default_width": 350,
        "show_diagnostics": "errors"
    },
    "debugger": {
        "button": false
    },
    "collaboration_panel": {
        "button": false
    },
    "search": {
        "button": false
    },
    "git_panel": {
        "button": false
    },
    "outline_panel": {
        "button": false
    },
    "notification_panel": {
        "button": false
    },
    "terminal": {
        "button": false
    },
    "agent": {
        "button": false,
        "always_allow_tool_actions": true,
        "default_profile": "write",
        "default_model": {
            "provider": "zed.dev",
            "model": "claude-sonnet-4"
        }
    },
    "features": {
        "edit_prediction_provider": "zed"
    },
    // "formatter": {
    //     "external": {
    //         "command": "./node_modules/.bin/biome",
    //         "arguments": [
    //             "format",
    //             "--write",
    //             "--stdin-file-path={buffer_path}"
    //         ]
    //     }
    // },
    "prettier": null,
    "ui_font_size": 29,
    "buffer_font_size": 31,
    "agent_font_size": 29,
    "buffer_font_family": ".ZedMono",
    "ui_font_family": ".ZedMono",
    "theme": "Retrofit Dark",
    "icon_theme": {
        "mode": "system",
        "light": "Zed (Default)",
        "dark": "Zed (Default)"
    },
    "use_smartcase_search": false,
    "seed_search_query_from_cursor": "selection",
    "show_completion_documentation": true,
    "always_treat_brackets_as_autoclosed": true,
    "experimental.theme_overrides": {
        "search.match_background": "#00BFBF40",
        "unreachable": "#00BFBF",
        "players": [
            {
                "cursor": "#0077CC",
                "background": "#0077CC",
                "selection": "#0099FF40"
            }
        ],
        "syntax": {
            "boolean": {
                "font_weight": 600,
                "color": "#8cbcae"
            },
            "number": {
                "font_weight": 600
            },
            "function": {
                "font_weight": 600
            },
            "string": {
                "font_style": "italic",
                "font_weight": 400
            },
            "variable.special": {
                "color": "#66B2FF",
                "font_style": null,
                "font_weight": null
            },
            "operator": {
                "color": "#808080",
                "font_style": null,
                "font_weight": null
            }
        },
        "element.selected": "#66B2FF30",
        "element.active": "#66B2FF20",
        "element.hover": "#66B2FF20",
        "border.focused": "#66B2FFFF",
        "border.selected": "#66B2FFCC",
        "pane.focused_border": "#66B2FFCC",
        "panel.focused_border": "#66B2FFCC",
        "tab.active_background": "#1A1A1A",
        "tab.inactive_background": "#00000000"
    }
}
