{
    "prettier": {
        // "allowed": true
    },
    "redact_private_values": false,
    "auto_signature_help": false,
    "auto_indent": true,
    "minimap": {
        "max_width_columns": 70
    },
    "show_signature_help_after_edits": false,
    "inlay_hints": {
        "enabled": false
    },
    "soft_wrap": "none",
    "hard_tabs": false,
    "active_pane_modifiers": {
        "inactive_opacity": 1.0
    },
    "centered_layout": {
        "left_padding": 0.19999980926513672
    },
    "bottom_dock_layout": "contained",
    "preview_tabs": {
        "enabled": true
    },
    "title_bar": {
        "show_menus": false,
        "show_sign_in": true,
        "show_user_picture": true,
        "show_onboarding_banner": true,
        "show_project_items": true,
        "show_branch_name": true
    },
    "diagnostics": {
        "button": true
    },
    "status_bar": {
        "cursor_position_button": true,
        "active_language_button": true
    },
    "file_finder": {
        "file_icons": false
    },
    "telemetry": {
        "diagnostics": false,
        "metrics": false
    },
    "auto_update": false,
    "edit_predictions": {
        "mode": "subtle",
        "enabled_in_text_threads": false
    },
    "tabs": {
        "file_icons": false,
        "git_status": false
    },
    "tab_size": 4,
    "tab_bar": {
        "show_nav_history_buttons": false,
        "show_tab_bar_buttons": false,
        "show": false
    },
    "toolbar": {
        "agent_review": true,
        "breadcrumbs": true,
        "quick_actions": false,
        "selections_menu": false,
        "code_actions": false
    },
    "cursor_blink": false,
    "vertical_scroll_margin": 2,
    "max_tabs": 5,
    "vim_mode": true,
    "vim": {
        "use_system_clipboard": "on_yank",
        "use_smartcase_find": true,
        "highlight_on_yank_duration": 200
    },
    "command_aliases": {
        "W": "w",
        "Wq": "wq",
        "Q": "q",
        "Qa": "qa",
        "Qwa": "qwa",
        "QWa": "qwa"
    },
    "relative_line_numbers": "enabled",
    "hide_mouse": "on_typing",
    "current_line_highlight": "all",
    "scrollbar": {
        "axes": {
            "vertical": false,
            "horizontal": false
        },
        "show": "never"
    },
    "gutter": {
        "line_numbers": true,
        "runnables": true,
        "breakpoints": true,
        "folds": true,
        "min_line_number_digits": 0
    },
    "indent_guides": {
        "enabled": true,
        "line_width": 0,
        "active_line_width": 2,
        "coloring": "indent_aware",
        "background_coloring": "disabled"
    },
    "git": {
        "inline_blame": {
            "enabled": true,
            "min_column": 0,
            "show_commit_summary": false
        },
        "hunk_style": "unstaged_hollow"
    },
    "project_panel": {
        "hide_root": true,
        "auto_fold_dirs": true,
        "starts_open": false,
        "file_icons": true,
        "entry_spacing": "standard",
        "dock": "right",
        "scrollbar": null,
        "button": false,
        "sticky_scroll": false,
        "folder_icons": false,
        "default_width": 385.0,
        "show_diagnostics": "errors"
    },
    "debugger": {
        "button": false
    },
    "collaboration_panel": {
        "button": false
    },
    "search": {
        "button": false
    },
    "git_panel": {
        "button": false
    },
    "outline_panel": {
        "file_icons": true,
        "button": false
    },
    "notification_panel": {
        "button": false
    },
    "terminal": {
        "button": false
    },
    "agent": {
        "default_profile": "write",
        "always_allow_tool_actions": true,
        "default_model": {
            "provider": "zed.dev",
            "model": "claude-sonnet-4-5"
        },
        "model_parameters": []
    },
    "features": {
        "edit_prediction_provider": "zed"
    },
    "ui_font_size": 30,
    "buffer_font_size": 36,
    "buffer_font_family": "DankMono Nerd Font Mono",
    "ui_font_family": "DankMono Nerd Font Mono",
    "theme": "Nord Dark",
    "icon_theme": {
        "mode": "system",
        "light": "Zed (Default)",
        "dark": "Zed (Default)"
    },
    "use_smartcase_search": true,
    "seed_search_query_from_cursor": "selection",
    "show_completion_documentation": true,
    "always_treat_brackets_as_autoclosed": true,
    "experimental.theme_overrides": {
        "vim.mode.text": "#2a2734",
        "vim.normal.background": "#98A1BC",
        "vim.insert.background": "#9BC09C",
        "vim.visual.background": "#ffe066",
        "vim.replace.background": "#ff6363",
        "vim.visual_line.background": "#F5C45E",
        "vim.visual_block.background": "#F79B72",
        "vim.helix_select.background": "#f5ede9",
        "vim.helix_normal.background": "#95b8d1",
        "players": [
            // {
            //     "cursor": "#2865ff",
            //     "background": "#0077CC",
            //     "selection": "#0099FF40"
            // }
        ]
